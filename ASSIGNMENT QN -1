//Write a query to fetch all the students from 2020 batch//
SELECT * FROM STUDENTS WHERE YEAR OF BATCH LIKE
'2020'OR'20%"
);

//Write a query to fetch all teachers from CS department//
SELECT * FROM TEACHERS WHERE DEPARTMENT LIKE
'CS'OR'CSE'
);

//Write a query to edit at least 3 records of students//
ALTER TABLE STUDENTS
MODIFY(NAME,.....NOT NULL,
       YEAR OF BATCH,...NOT NULL,
       REG NO,...NOT NULL)
);

//Write a query to delete 2 records from teachers table.//
DELETE TOP(20)PERCENT 
FROM TEACHERS,TEACHERS
         OR
DELETE FROM TEACHERS WHERE ID IN 
(SELECT * FROM TABLE WHERE ID=' ')
);

//Create a table for students with name,age,mobile,regno.,yearofbatch as columns//
Create TABLE IF NOT EXISTS STUDENTS(
Student_Name Varchar(40)NOT NULL,
Mobile  INT (15) NOTNULL,
Reg no INT(15) NOTNULL,
year of batch YEAR,
Age Varchar(2),
);

// Create a table for teachers with name, domain, department as column//
Create TABLE IF NOT EXISTS Teachers(
Name Varchar(40) NOT NULL,
Domain Varchar(40) NOT NULL,
Department Varchar(40) NOTNULL,
);

